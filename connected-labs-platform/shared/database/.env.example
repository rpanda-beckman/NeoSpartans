# Sample environment configuration for database connection
# Copy this to .env and update with your actual database credentials

DATABASE_URL=postgresql://username:password@localhost:5432/connected_labs
REDIS_URL=redis://localhost:6379/0

# Database connection pool settings
DB_POOL_MIN=5
DB_POOL_MAX=20
DB_POOL_IDLE_TIMEOUT=30000

# Redis settings for caching
REDIS_TTL=3600
REDIS_KEY_PREFIX=connected_labs: